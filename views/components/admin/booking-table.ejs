<div class="card shadow mb-4">
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>No</th>
            <th>Invoice</th>
            <th>Date</th>
            <th>Title</th>
            <th>Name Member</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% for(let i = 0; i < bookings.length; i++){ %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= bookings[i].invoice %></td>
            <td><%= bookings[i].startDate.toLocaleString().split(',')[0] %> -
              <%= bookings[i].endDate.toLocaleString().split(',')[0] %></td>
            <td><%= bookings[i].itemId.title %></td>
            <td><%= bookings[i].memberId.firstName %> <%= bookings[i].memberId.lastName %></td>
            <td><%= bookings[i].payments.status %></td>
            <td>
              <a href="/admin/booking/<%= bookings[i]._id %>" class="btn btn-success btn-circle btn-sm button-update">
                <i class="fas fa-eye"></i>
              </a>
            </td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>